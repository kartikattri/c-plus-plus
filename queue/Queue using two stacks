#include <iostream>
#include<stack>
using namespace std;

class Queue{
    public:
    stack<int> s1;
    stack<int> s2;
    int n;
    
    void Push(int x){
        while(!s2.empty()){
            s1.push(s2.top());
            s2.pop();
        }
        if(s1.empty()){
            n=x;
        }
        s1.push(x);
    }
    
    void Pop(){
        while(!s1.empty()){
            s2.push(s1.top());
            s1.pop();
        }
        s2.pop();
        if(!s2.empty()){
            n=s2.top();
        }
    }
    
    int Front(){
        return n;
    }
};

int main() {
    int q;
    cin>>q;
    Queue s;
    while(q--){
        int qry;
        cin>>qry;
        
        if(qry==1){
            int x;
            cin>>x;
            s.Push(x);
        }
        else if(qry==2){
            s.Pop();
        }
        else if(qry==3){
            cout<<s.Front()<<endl;
        }
    }
	return 0;
}
